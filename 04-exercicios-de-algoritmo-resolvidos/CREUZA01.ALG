algoritmo "Creuza01"
var
   ano_atual, ano_nascimento, idade: Inteiro
inicio
      Escreva("Em que ano nós estamos?: ")
      Leia(ano_atual)
      Escreva("Em que ano eu nasci?: ")
      Leia(ano_nascimento)
      
      idade <- ano_atual - ano_nascimento
      
      Escreval()
      Escreva("Minha idade será ", idade)
fimalgoritmo