algoritmo "pratica2"

var
   numero: Real
inicio
      Escreva("Informe um número: ")
      Leia(numero)
      
      Escreval()
      Escreval("Valor absoluto: ", Abs(numero))
      Escreval("Inteiro: ", Int(numero))
      Escreval("Potência: ", Exp(numero, 2))
      Escreval("Raiz: ", RaizQ(numero))
      
fimalgoritmo
